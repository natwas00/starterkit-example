{
    "_version": "1.65.0",
    "sap.app": {
        "id": "stk.starterkit",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.3",
            "toolsId": "6399f77c-198b-4319-a35a-ad0090721772"
        },
        "dataSources": {
            "mainService": {
                "uri": "/V2/Northwind/Northwind.svc/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "secondaryService":{
                "uri": "generate_insult.php",
                "type": "JSON"
            
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.132.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "stk.starterkit.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "useBatch": false
                }
            },
            "secondaryModel": {
                "dataSource": "secondaryService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "useBatch": false
                }
            }
    },

        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "stk.starterkit.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
              },
            "routes": [
                {
                    "name": "CustomerList",
                    "pattern": "",
                    "target": [
                      "TargetCustomerList"
                    ]
                  },
                  {
                    "name":"CustomerDetails",
                    "pattern":"CustomerDetails/{CustomerID}",
                    "target":[
                      "TargetCustomerDetails"
                    ]
                  },
                  {
                    "name":"CreateCustomer",
                    "pattern":"CreateCustomer",
                    "target":[
                      "TargetCreateCustomer"
                    ]
                  },
                  {
                    "name":"EmployeeList",
                    "pattern":"EmployeeList",
                    "target":[
                      "TargetEmployeeList"
                    ]
                  }
            ],
            "targets": {
                "TargetCustomerDetails":{
                    "viewType": "XML",
                    "viewId": "CustomerDetails",
                    "viewName": "CustomerDetails"
                  },
                  "TargetCustomerList":{
                    "viewType": "XML",
                    "viewId": "CustomersList",
                    "viewName": "CustomersList"
                  },
                  "TargetCreateCustomer":{
                    "viewType": "XML",
                    "viewId": "CreateCustomer",
                    "viewName": "CreateCustomer"
                },
                "TargetEmployeeList":{
                    "viewType": "XML",
                    "viewId": "EmployeeList",
                    "viewName": "EmployeeList"
                  }
            }
        },
        "rootView": {
            "viewName": "stk.starterkit.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    }
}